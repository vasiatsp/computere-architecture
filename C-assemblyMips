.data
AddrConstant5381: .word 5381   # Store the constant 5381 in memory
data_array: .word 3, 4, 5, 6
array_size: .word 4
tree: .space 16 
.text
main: 

create_leaves: 

la $a0, data_array
la $a1, array_size
la $a2, tree
move $t1, $zero


LOOP:
slt $t2, $t1, $a0 
beq $t2, $zero, END
jal cslab_hash
sw $a3, 0($a2)
addi $a0, $a0, 4
addi $a2, $a2, 4
addi $t1, $t1, 1
move $a3, $v0
move $a0, $a3
li $v0, 1
syscall
j LOOP 

END: 
